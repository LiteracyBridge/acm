<?xml version="1.0" encoding="MacRoman" ?>
<persistence xmlns="http://java.sun.com/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd"
             version="1.0">
  <persistence-unit name="lbPersistenceUnit" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>org.literacybridge.acm.db.PersistentAudioItem</class>
    <class>org.literacybridge.acm.db.PersistentCategory</class>
    <class>org.literacybridge.acm.db.PersistentLocale</class>
    <class>org.literacybridge.acm.db.PersistentLocalizedAudioItem</class>
    <class>org.literacybridge.acm.db.PersistentManifest</class>
    <class>org.literacybridge.acm.db.PersistentMetadata</class>
    <class>org.literacybridge.acm.db.PersistentReferencedFile</class>
    <class>org.literacybridge.acm.db.PersistentObject</class>
    <class>org.literacybridge.acm.db.PersistentLocalizedString</class>
    <class>org.literacybridge.acm.db.PersistentString</class>
  </persistence-unit>
</persistence>
